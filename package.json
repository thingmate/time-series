{
  "name": "@thingmate/time-series",
  "version": "0.0.1",
  "type": "module",
  "main": "./index.js",
  "scripts": {
    "fb:build": "node fabrique/actions/cli/build.cli.js",
    "fb:format": "prettier --write --ignore-unknown --config ./.prettierrc \"src/**/*.{js,jsx,ts,tsx,html,css,scss}\"",
    "fb:test": "vitest",
    "fb:test:coverage": "vitest run --coverage",
    "fb:bench": "vitest bench",
    "fb:typedoc": "typedoc --options typedoc.json --tsconfig tsconfig.json",
    "fb:prod": "node fabrique/actions/cli/build-and-publish.cli.js --mode prod",
    "fb:dev": "node fabrique/actions/cli/build-and-publish.cli.js --mode dev",
    "fb:rc": "node fabrique/actions/cli/build-and-publish.cli.js --mode rc",
    "fb:node:dev": "node --experimental-strip-types --watch .debug/main.ts",
    "fb:node:dev:inspect": "node --experimental-strip-types --watch --inspect .debug/main.ts"
  },
  "description": "Time series",
  "keywords": [],
  "author": "Valentin Richard",
  "license": "MIT",
  "access": "public",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thingmate/time-series.git"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitest/coverage-istanbul": "^4.0.14",
    "prettier": "^3.7.3",
    "prettier-plugin-organize-imports": "^4.3.0",
    "typedoc": "^0.28.15",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "packageManager": "yarn@4.12.0",
  "fabrique": {
    "version": "1.0.20",
    "type": "lib"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.6",
    "@hono/standard-validator": "^0.2.0",
    "@xstd/path": "^1.0.0",
    "fastify": "^5.6.2",
    "hono": "^4.10.7",
    "zod": "^4.1.13"
  }
}
